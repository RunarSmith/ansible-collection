
# HackTheBox:
# - hostname: *.htm
# - IP: 10.10.*
# TryHackMe:
# - hostname: *.thm
# - IP: 10.10.*
#
# equivalent to :
# ssh -o StrictHostKeyChecking=no -o UserKnownHostFiles=/dev/null user@host
Host 10.10.* *.htm *.thm
    StrictHostKeyChecking   no
    UserKnownHostsFile      /dev/null
    HostKeyAlgorithms       ssh-rsa
